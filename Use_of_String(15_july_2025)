        "# Single quote string\n",
        "single_quote_string = 'This is a string in single quotes.'\n",
        "print(single_quote_string)\n",
        "\n",
        "# Double quote string\n",
        "double_quote_string = \"This is a string in double quotes.\"\n",
        "print(double_quote_string)\n",
        "\n",
        "# Using single quotes inside double quotes\n",
        "mix1 = \"It's a beautiful day!\"\n",
        "print(mix1)\n",
        "\n",
        "# Using double quotes inside single quotes\n",
        "mix2 = 'He said, \"Hello!\" to everyone.'\n",
        "print(mix2)\n",
        "\n",
        "# Triple single quotes for multi-line string\n",
        "multi_line1 = '''This is a string\n",
        "written in triple single quotes.\n",
        "It can go to the next line easily.'''\n",
        "print(multi_line1)\n",
        "\n",
        "# Triple double quotes for multi-line string\n",
        "multi_line2 = \"\"\"This is another string\n",
        "written in triple double quotes.\n",
        "Used for long texts or documentation.\"\"\"\n",
        "print(multi_line2)\n"
      ]
